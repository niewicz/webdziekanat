<!DOCTYPE html>
<html>
  <head>
    <title>TUL | Web dean's office</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <%= render 'shared/navbar' %>

    <div class="grid below-navbar">

      <div class="grid-item 1/5 medium-hidden compact-hidden">
        <div class="grid-item 1/1">
          <%= render 'shared/useful_links' %>
        </div>
        <div class="grid-item 1/1">
          <%= render 'shared/timetable_changes_last' %>
        </div>
      </div>

      <div class="grid-item 4/5 medium-1/1 compact-1/1">
        <%= yield %>
      </div>

    </div>
    
  </body>
</html>
